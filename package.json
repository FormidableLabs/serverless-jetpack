{
  "name": "serverless-jetpack",
  "version": "0.0.1",
  "description": "A faster JavaScript packager for Serverless applications.",
  "main": "index.js",
  "repository": "https://github.com/FormidableLabs/serverless-jetpack",
  "author": "Ryan Roemer <ryan.roemer@formidable.com>",
  "license": "MIT",
  "keywords": [
    "serverless",
    "serverless artifact",
    "serverless package",
    "serverless plugin",
    "amazon web services",
    "aws",
    "lambda",
    "npm",
    "yarn"
  ],
  "scripts": {
    "preversion": "yarn check",
    "usage": "cd test/packages/simple/yarn &&  MODE=yarn serverless jetpack -h",
    "clean": "rm -rf test/packages/*/*/.serverless",
    "benchmark:install": "node test/script.js install",
    "benchmark:build": "node test/script.js build",
    "benchmark:test": "mocha test/benchmark.js",
    "benchmark": "node test/script.js benchmark",
    "test:spec": "mocha test/spec",
    "lint": "eslint .",
    "test": "yarn test:spec",
    "check": "yarn lint && yarn test:spec"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "globby": "^9.2.0",
    "nanomatch": "^1.2.13",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "eslint": "^5.13.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-promise": "^4.0.1",
    "markdown-table": "^1.1.2",
    "mocha": "^6.1.4",
    "mock-fs": "^4.9.0",
    "serverless": "^1.41.1",
    "strip-ansi": "^5.2.0"
  }
}
